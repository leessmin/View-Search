{
    "name": "view search",
    "description": "Precise search keywords",
    "version": "0.1.2",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html",
        "default_title": "search",
        "default_icon": {
            "16": "/images/get_started16.png",
            "32": "/images/get_started32.png",
            "48": "/images/get_started48.png",
            "128": "/images/get_started128.png"
        }
    },
    "icons": {
        "16": "/images/get_started16.png",
        "32": "/images/get_started32.png",
        "48": "/images/get_started48.png",
        "128": "/images/get_started128.png"
    },
    "permissions": [
        "notifications",
        "declarativeContent",
        "activeTab",
        "scripting",
        "contentSettings",
        "unlimitedStorage",
        "tabs",
        "webRequest"
    ],
    "host_permissions": [
        "<all_urls>"
    ]
}